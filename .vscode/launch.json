    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "executable": "quicksort",
            "type": "gdb",
            "request": "attach",
            "remote": true,
            "debugger_args": [ "quicksort" ],
            "target": "localhost:2323",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
            "gdbpath": "/usr/share/risc-v/bin/riscv64-unknown-elf-gdb"
        }
    ]
}